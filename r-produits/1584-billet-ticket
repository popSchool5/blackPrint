																	} resultat=new Array(prix_transport,valdelai,valcommentaire); return resultat;  } function determine_zone_grille_28(infoclient,masse,type) { prix_transport=0; valcommentaire=''; valdelai=0;id_zone_charge=false; if (type.indexOf('id_zone_charge',0)>=0) id_zone_charge=true; test_trouve_zone=false; if (type.indexOf('test_trouve_zone',0)>=0) test_trouve_zone=true; grille_transport=new Array(); grille_transport.poids_mini=Array(0,"0","10.01"); grille_transport.id_zone=Array('',"3"); grille_transport.poids_maxi=Array(0,"10","99999.99999"); grille_transport.prix_forfaitaire=Array(Array(0,0,0) ,Array(0,"0.00","0.00") ); grille_transport.unite_poids=Array(0,"0","0"); grille_transport.prix_unite_poids=Array(Array(0,0,0) ,Array(0,"0.00","0.00") ); grille_transport.descriptif_zone=Array('',"3"); grille_transport.commentaire=Array(Array("","","") ,Array("","","") ); grille_transport.delai=Array(Array(0,0,0) ,Array(0,"0.00","0.00") ); xmax=2;  ymax=1; if (id_zone_charge) { id_zone=infoclient; } else { id_zone=$('#VAR_1_').val();  } if(test_trouve_zone) { if(in_array(id_zone,grille_transport.id_zone)) { return(true);} else { return(false); } } sortie=false;	y=0; id_zone_universelle=0;
