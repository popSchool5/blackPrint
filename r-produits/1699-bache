																													                        <li class="li-niveau1 advtm_menu_11 advtm_search menuHaveNoMobileSubMenu"
                            data-js-swap-id="menu-search"                        >
                            <div class="adtm_search_container">                                                                    <div class="mobile_search"></div>
<div class="search_container">
    <form method="get" action="https://www.eoz.eu/recherche" class="searchboxATM">
        <label for="search_query_atm_menu_11"></label>
        <input type="hidden" name="controller" value="search" />
        <input type="hidden" name="orderby" value="position" />
        <input type="hidden" name="orderway" value="desc" />
        <input type="text"
               class="search_query_atm"
               id="search_query_atm_menu_11"
               name="search_query"
               placeholder="Rechercher"
                onfocus="javascript:if(this.value=='Recherche')this.value='';" onblur="javascript:if(this.value=='')this.value='Recherche';" />
                    <button type="submit" name="submit_search" class="btn btn-default button-search">
                <span>Rechercher</span>
            </button>
            </form>
</div>
	<script type="text/javascript">
		$("#search_query_atm_menu_11").autocomplete(
			"https://www.eoz.eu/recherche", {
				minChars: 3,
				max: 10,
				width: 500,
				selectFirst: false,
				scroll: false,
				dataType: "json",
				formatItem: function(data, i, max, value, term) { return value;	},
				parse: function(data) {
					var mytab = new Array();
					for (var i = 0; i < data.length; i++)
					mytab[mytab.length] = { data: data[i], value: data[i].cname + ' > ' + data[i].pname };
					return mytab;
				},
				extraParams: {
					ajaxSearch: 1,
					id_lang: 1
				}
			}
		).result(function(event, data, formatted) {
			$('search_query_atm_menu_11').val(data.pname);
			document.location.href = data.product_link;
		});
	</script>

                                                                                                                            </div>                        </li>
                    														
