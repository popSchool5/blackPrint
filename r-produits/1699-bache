<script> var grille_transport_a_utiliser=false; 
if (!String.prototype.replaceAll) {
	String.prototype.replaceAll = function(str, newStr){
		if (Object.prototype.toString.call(str).toLowerCase() === '[object regexp]') {
			return this.replace(str, newStr);
		}
		return this.replace(new RegExp(str, 'g'), newStr);
	};
}
var tableau_pays_utilisateur_realisaprint=""; var tableau_des_remises_du_client={ }; 
function ajustement_prix_delaiNUMERO_EN_COURS(phrase_code,test,qte_testee) { coefficient=1; delta_delai=0; tranche_trouvee=false; if( phrase_code.indexOf('euro')>=0) seuil_reference=test; else seuil_reference=qte_testee; if( phrase_code.indexOf('expert')<0  ) { if (is_numeric(phrase_code)) { coefficient=floatval(phrase_code); tranche_trouvee=true; } } else { tableau_grille=[];
			t_inter=explode(String.fromCharCode(167),phrase_code);
			ymax=-1; xmax=3;
			for(x=0;x<=xmax; x++) { tableau_grille[x]=[]; }
			for(var index in t_inter) { element=t_inter[index];
					t_inter2=explode('https://www.eoz.eu/',element);
					if(t_inter2.length>2)
						{
						ymax++;
						for(x=0;x<=xmax; x++) { tableau_grille[x][ymax]=floatval(t_inter2[x]); }
						}
					} 
			seuil_reference=floatval(seuil_reference); 
			if(ymax>=0)
				{ 
				for(y=ymax; y>=0; y--)
					{
					ok=(!tranche_trouvee && seuil_reference >= tableau_grille[0][y]);
					if(ok && trim( tableau_grille[1][y])!='') ok=(seuil_reference <= tableau_grille[1][y]); 
					if(ok) { coefficient=tableau_grille[2][y]; delta_delai=tableau_grille[3][y]; tranche_trouvee=true; } 
					} 
				} 
		}
if (tranche_trouvee) return [coefficient,delta_delai];
				else return false;
} 
		function initProgress() {
			$("div#magazine_qty_alert").hide();
			var bar = $('<div id="progressbar"></div>').text("");
			var label = $('<div class="progress-label tac p5"></div>').css({"width": "100%", "max-width": "100%"}).text("Calcul des configurations possibles... ");
			var span = $('<span></span>').text("0%");
			label.append(span);
			bar.append(label);
			$("div#overlay_inner div#overlay_inner_inner").empty().append(bar);
			$("div#overlay_inner div#overlay_inner_title").html("<img class=\"img-responsive\" src=\"https://www.realisaprint.com/site/charte/images/tb/think.gif\" alt=\"Calcul en cours. Veuillez patienter...\" >");
			$("div#overlay").show();
			$("div#overlay_inner").show();
			$("div#overlay_loading").hide();
			bar.progressbar({
				value: 0,
				change: function() {
					span.text(bar.progressbar("value") + "%");
				},
			});
			var value = bar.find(".ui-progressbar-value");
			value.css("background", "#2AAF52");
			value.css("margin", "-26px 0");
		}
	
		function progress() {
			var bar = $("div#progressbar");
			if (bar.hasClass("done"))
				return;
			var val = bar.progressbar("value") || 0;
			bar.progressbar("value", val + 1);
			if (val < 98) {
				var time = Math.round(val * 10 + 300 + (250 * Math.random()));
				setTimeout(function() {
					progress();
				}, time);
			}
		}
	
		function endProgress(valid) {
			var bar = $("div#progressbar");
			bar.addClass("done");
			var value = bar.find(".ui-progressbar-value");
			var label = $("div#progressbar div.progress-label");
			label.html("Configurations calculées. Mise en forme du tableau... <span>" + bar.progressbar("value") + "%</span>");
			var val = bar.progressbar("value") || 0;
			bar.progressbar("value", val + 1);
			if (val < 100) {
				setTimeout(function() {
					endProgress(valid);
				}, 5);
				return;
			}
			if (valid == true) {
				setTimeout(function() {
					label.text("Configurations calculées ! La fenêtre va se fermer.");
					label.addClass("txt-white");
					value.css("background", "#2AAF52");
					setTimeout(function() {
						$("div#overlay").hide();
					}, 1000);
				}, 1000);
			} else {
				var html_text_1 = $("<p style=\"font-size: 18px; font-size: 1.8rem; font-weight: 400;\"></p>").addClass("tac").html("<span style=\"font-size: 34px; font-size: 3.4rem; font-weight: 500; margin-right: 5px; margin-right: 0.5rem;\">Oups !</span> Petite erreur de calcul");
				var html_text_2 = $("<p></p>").addClass("tac man").html("<i class=\"far fa-redo-alt\" style=\"font-size: 12px; font-size: 1.2rem; margin-right: 10px; margin-right: 1rem;\"></i><span>La page va être rechargée</span>");
				$("div#overlay_inner_inner").empty().append(html_text_1).append(html_text_2);
				setTimeout(function() {
					location.reload();
				}, 5000);
			}
		}
	function genere_tableau_de_prix_NUMERO_EN_COURS() { 
	var magazine_config = false;
	var quantite_affichee_perso = false
if (magazine_config != false) {
	for (var key in magazine_config) {
		qte = parseInt(key);
		if (liste_qtes_en_cours[0].indexOf(qte) == -1) {
			liste_qtes_en_cours[0].push(qte);
		}
		if (qtes_from_ajax.indexOf(qte) == -1) {
			qtes_from_ajax.push(qte);
		}
	}
	liste_qtes_en_cours[0].sort((a, b) => a - b);
	qtes_from_ajax.sort((a, b) => a - b);
} else {
